{
	"name": "ason_cs_ts",
	"version": "0.1.0",
	"description": "Ason CS TS",
	"private": true,
	"scripts": {
		"build": "turbo run build",
		"start": "cd apps/frontend && npm run start",
		"dev": "turbo run dev",
		"lint": "turbo run lint",
		"format": "prettier --write \"**/*.{ts,tsx,md}\"",
		"check-types": "turbo run check-types"
	},
	"devDependencies": {
		"prettier": "^3.5.3",
		"turbo": "^2.5.0",
		"typescript": "5.8.2"
	},
	"engines": {
		"node": ">=22"
	},
	"packageManager": "npm@10.9.2",
	"workspaces": [
		"apps/*",
		"packages/*",
		"packages/*/*"
	]
}
